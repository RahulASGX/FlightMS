<web-app>

<display-name>Welcome to Tomcat</display-name>
<description>
Welcome to Tomcat
</description>
<servlet>
<servlet-name>searchServlet</servlet-name>
<servlet-class>com.test.Search</servlet-class>
</servlet>
<servlet>
