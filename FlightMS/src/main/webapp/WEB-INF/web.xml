	<servlet-class>com.test.Pay</servlet-class>
</servlet>
<servlet>
	<servlet-name>test</servlet-name>
	<display-name>test</display-name>
	<description></description>
	<servlet-class>test</servlet-class>
</servlet>
<servlet>
	<servlet-name>index</servlet-name>
	<display-name>index</display-name>
	<description></description>
	<servlet-class>com.zetcode.util.index</servlet-class>
</servlet>
<servlet>
	<servlet-name>JavaPdfHelloWorld</servlet-name>
	<display-name>JavaPdfHelloWorld</display-name>
	<description></description>
	<servlet-class>com.zetcode.util.JavaPdfHelloWorld</servlet-class>
</servlet>

<servlet-mapping>
<servlet-name>searchServlet</servlet-name>
<url-pattern>/search</url-pattern>
</servlet-mapping>

<servlet-mapping>
	<servlet-name>Running_status</servlet-name>
	<url-pattern>/Running_status</url-pattern>
</servlet-mapping>
<servlet-mapping>
	<servlet-name>Flight_id_Search</servlet-name>
