	<description></description>
	<servlet-class>com.test.Running_stat</servlet-class>
</servlet>
<servlet>
	<servlet-name>Running_status</servlet-name>
	<display-name>Running_status</display-name>
	<description></description>
	<servlet-class>com.test.Running_status</servlet-class>
</servlet>

<servlet>
	<servlet-name>Flight_id_Search</servlet-name>
	<display-name>Flight_id_Search</display-name>
	<description></description>
	<servlet-class>com.test.Flight_id_Search</servlet-class>
</servlet>
<servlet>
	<servlet-name>Book</servlet-name>
	<display-name>Book</display-name>
	<description></description>
	<servlet-class>com.test.Book</servlet-class>
</servlet>
<servlet>
	<servlet-name>Block</servlet-name>
	<display-name>Block</display-name>
	<description></description>
	<servlet-class>com.test.Block</servlet-class>
</servlet>
<servlet>
	<servlet-name>Pay</servlet-name>
